{
  "name": "english-essence-chrome-extension",
  "version": "1.0",
  "description": "Add-on for adding words to english-essence app",
  "background": {
    "scripts": ["contextMenu.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "contextMenus", "notifications", "http://172.20.1.1/*", "https://jsonplaceholder.typicode.com/*"],
  "manifest_version": 2
}